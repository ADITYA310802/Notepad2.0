<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>SaveNoteServ</servlet-name>
  	<display-name>SaveNoteServ</display-name>
  	<description></description>
  	<servlet-class>com.servs.SaveNoteServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DeleteServ</servlet-name>
  	<display-name>DeleteServ</display-name>
  	<description></description>
  	<servlet-class>com.servs.DeleteServ</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>UpdateServ</servlet-name>
  	<display-name>UpdateServ</display-name>
  	<description></description>
  	<servlet-class>com.servs.UpdateServ</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>SaveNoteServ</servlet-name>
  	<url-pattern>/SaveNoteServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DeleteServ</servlet-name>
  	<url-pattern>/DeleteServ</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>UpdateServ</servlet-name>
  	<url-pattern>/UpdateServ</url-pattern>
  </servlet-mapping>
</web-app>
